/* Keep the new logo visible/centered */
#hero-logo{display:block;margin:10px auto;max-height:110px}

/* Center the Tech Support button under the logo */
.btn-support-wrap{width:100%;display:flex;justify-content:center;margin:12px 0 18px;clear:both}
#btn-tech-support{display:inline-block;text-align:center}

/* If anything else with “logo” sneaks in, hide it */
img[src*="logo"]:not(#hero-logo),
picture img[src*="logo"]:not(#hero-logo),
img[alt*="logo" i]:not(#hero-logo),
svg[aria-label*="logo" i]:not(#hero-logo){display:none !important}
